@model WebGB.Models.Employeer

@{
    ViewData["Title"] = "DispEmployer";
}
<h1>Карточка сотрудника</h1>
<table>
    <tr>
        <td>ID</td>
        <td> @Model.ID </td>
    </tr>
    <tr>
        <td>Имя</td>
        <td> @Model.Name</td>
    </tr>
    <tr>
        <td>Фамилия</td>
        <td> @Model.Surname</td>
    </tr>
    <tr>
        <td>Дата</td>
        <td>@Model.Date</td>
    </tr>
    <tr>
        <td>Описание</td>
        <td>@Model.Description</td>
    </tr>
</table>
<a href='~/Default/'>Домой</a>
